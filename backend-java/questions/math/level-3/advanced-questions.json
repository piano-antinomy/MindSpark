      "latex_choices": [
        "$\\textbf{(A)}\\ x = 1, 6 \\qquad \\textbf{(B)}\\ x = 2, 3 \\qquad \\textbf{(C)}\\ x = -2, -3 \\qquad \\textbf{(D)}\\ x = 0, 5$"
      ],
      "asy_choices": []
    },
    "tags": ["algebra", "quadratic-equations"],
    "sources": ["MindSpark Level 3"],
    "answer": "B",
    "solutions": [
      {
        "solution_id": 1,
        "type": "text",
        "value": ["Factor: x² - 5x + 6 = (x - 2)(x - 3) = 0. Therefore x = 2 or x = 3."]
      }
